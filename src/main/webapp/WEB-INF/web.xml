<web-app>
	<servlet>
		<servlet-name>fast</servlet-name>
		<servlet-class>org.springframework.web.servlet.DispatcherServlet
		</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>

	<servlet-mapping>
		<servlet-name>fast</servlet-name>
		<url-pattern>/</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
	<welcome-file>/WEB-INF/jsps/generateOtp.jsp</welcome-file>
	</welcome-file-list>

</web-app>